{
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/socket" }
    ],
    "headers": [
      {
        "source": "/api/socket",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,POST" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
        ]
      }
    ]
  }